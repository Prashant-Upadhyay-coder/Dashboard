
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] </title>
    <link href="~/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/css/DataTable/dataTables.bootstrap5.min.css" rel="stylesheet" />
    @*<link href="~/css/site.css" rel="stylesheet" />*@
    <link href="~/css/all.min.css" rel="stylesheet" />
    <link rel="shortcut icon" type="image/x-icon" href="~/images/favicon.png">
    <link href="~/css/fontawesome.min.css" rel="stylesheet" />
    <link href="~/css/bootstrap-icons.min.css" rel="stylesheet" />
    <link href="~/DTPicker/jquery.datetimepicker.min.css" rel="stylesheet" />
    <script src="~/js/jquery-3.6.0.js"></script>
    <style>
        body {
            background-image: url("/images/login.jpg") !important;
            background-size: cover !important;
            background-repeat: no-repeat !important;
            background-color: transparent !important;
            height:100vh;
        }
    </style>
</head>
<body>
    <div class="container-fluid image_set">
        @*@Html.Partial("header")*@
        @*@Html.Partial("SideMenu")*@
        @RenderBody()
    </div>
    <script src="~/js/DataTable/jquery.dataTables.min.js"></script>
    <script src="~/js/DataTable/dataTables.bootstrap5.min.js"></script>
    <script src="~/js/moment.min.js"></script>
    <script src="~/DTPicker/jquery.datetimepicker.full.min.js"></script>
    <script src="~/js/jquery.slimscroll.min.js"></script>
    <script src="~/js/jquery.validate.js"></script>
    <script src="~/js/TectonaValidate.js"></script>
    <script src="~/js/popper.min.js"></script>
    <script src="~/js/bootstrap.min.js"></script>
    <script src="~/js/DataTable/Bootstrap_class.js"></script>
    <script src="~/js/app.js"></script>
    <script>
        $('.datepicker').datetimepicker({
            timepicker: false,
            format: "d/m/Y",
        });
    </script>
    @RenderSection("Scripts", required: false)
</body>
</html>
